        'mih-session.cc',
        'mih-function.cc',
        'mih-protocol.cc',
        'simple-mih-link-sap.cc',
        'simple-mih-user.cc',
        ]
    headers = bld.new_task_gen('ns3header')
    headers.module = 'mih'
    headers.source = [
        'mih-tlv.h',
        'mih-callbacks.h',
        'mih-tlv-object-factory.h',
