## -*- Mode: python; py-indent-offset: 4; indent-tabs-mode: nil; coding: utf-8; -*-

def build(bld):
    obj = bld.create_ns3_module('mih', ['simulator', 
                                        'core'])
    obj.source = [
        'mih-tlv.cc',
        'mih-bandwidth.cc',
        'mih-device-states-response.cc',
        'mih-battery-level.cc',
        'mih-capability-flag.cc',
